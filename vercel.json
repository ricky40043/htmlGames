{
  "version": 2,
  "name": "mini-games-collection",
  "builds": [
    {
      "src": "**/*.html",
      "use": "@vercel/static"
    },
    {
      "src": "**/*.js",
      "use": "@vercel/static"
    },
    {
      "src": "**/*.css",
      "use": "@vercel/static"
    },
    {
      "src": "**/*.mp3",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/bomb-topic",
      "dest": "/bomb-topic/index.html"
    },
    {
      "src": "/guess-number",
      "dest": "/guess-number/index.html"
    },
    {
      "src": "/two-types-people",
      "dest": "/two-types-people/index.html"
    },
    {
      "src": "/two-player-quiz",
      "dest": "/two-player-quiz/index.html"
    },
    {
      "src": "/who-is-spy",
      "dest": "/who-is-spy/index.html"
    },
    {
      "src": "/2048",
      "dest": "/CascadeProjects/2048/index.html"
    }
  ],
  "headers": [
    {
      "source": "**/*.mp3",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}